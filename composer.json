{
    "name": "ekino/phpstan-sonata",
    "type": "phpstan-extension",
    "description": "PHPStan extension for Sonata",
    "keywords": [
        "PHPStan",
        "Sonata Project"
    ],
    "homepage": "https://github.com/ekino/phpstan-sonata",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi Marseille",
            "email": "remi.marseille@ekino.com",
            "homepage": "https://www.ekino.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "phpstan/phpstan": "^0.12"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "nikic/php-parser": "^4.3",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^8.5",
        "sensiolabs/security-checker": "^6.0",
        "sonata-project/datagrid-bundle": "^3.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Ekino\\PHPStanSonata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Ekino\\PHPStanSonata\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
